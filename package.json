{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "server": "nodemon server.js",
    "sass": "sass --watch static/scss:static/css",
    "sass-build": "sass static/scss:static/css",
    "scss": "yarn sass",
    "parcel-watch": "parcel watch static/index.html -d static/dist",
    "parcel-build": "parcel build static/index.html -d static/dist",
    "start": "concurrently \"yarn sass\" \"yarn parcel-watch\" \"yarn server\"",
    "prod": "concurrently \"yarn sass-build\" \"yarn parcel-build\" "
  },
  "license": "MIT",
  "dependencies": {
    "compression": "^1.7.4",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "concurrently": "^6.0.2",
    "cors": "^2.8.5",
    "sass": "^1.32.10"
  }
}
